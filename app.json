      "adaptiveIcon": {
        "foregroundImage": "./assets/mt_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ticketingscanner",
      "intentFilters": [],
      "config": {
        "cleartextTraffic": true
      },
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/mt_icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fb4f1904-11fd-4bce-aa77-a6b6c997d189"
      }
    },
    "owner": "ichizawa"
  }
